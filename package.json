{
  "name": "redux-portfolio",
  "version": "1.0.0",
  "description": "Custom webpack build using React, Redux, and json-server",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server --config webpack.config.dev.js --port 3000",
    "test": "jest --watch"
  },
  "jest": {
    "setupFiles": [
      "./tools/testSetup.jsx"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/tools/fileMock.js",
      "\\.(css|less)$": "<rootDir>/tools/styleMock.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DopamineDriven/redux-portfolio.git"
  },
  "keywords": [],
  "author": "Andrew Ross",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/DopamineDriven/redux-portfolio/issues"
  },
  "homepage": "https://github.com/DopamineDriven/redux-portfolio#readme",
  "dependencies": {
    "bootstrap": "^4.4.1",
    "immer": "^6.0.3",
    "path": "^0.12.7",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-inlinesvg": "^1.2.0",
    "react-redux": "^7.2.0",
    "react-router-dom": "^5.1.2",
    "react-toastify": "^5.5.0",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "reselect": "^4.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@testing-library/react": "^10.0.3",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "babel-preset-react-app": "^9.1.2",
    "css-loader": "^3.5.3",
    "cssnano": "^4.1.10",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint": "^6.8.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-react": "^7.19.0",
    "fetch-mock": "^9.4.0",
    "file-loader": "^6.0.0",
    "html-webpack-plugin": "^4.2.0",
    "http-server": "^0.12.3",
    "jest": "^25.4.0",
    "json-server": "^0.16.1",
    "mini-css-extract-plugin": "^0.9.0",
    "node-fetch": "^3.1.1",
    "npm-run-all": "^4.1.5",
    "postcss-loader": "^3.0.0",
    "react-test-renderer": "^16.13.1",
    "redux-immutable-state-invariant": "^2.1.0",
    "redux-mock-store": "^1.5.4",
    "rimraf": "^3.0.2",
    "style-loader": "^1.2.0",
    "svg-inline-loader": "^0.8.2",
    "webpack": "^4.43.0",
    "webpack-bundle-analyzer": "^3.7.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3"
  },
  "engines": {
    "node": "12.16.2",
    "npm": "6.14.4"
  },
  "babel": {
    "presets": [
      "babel-preset-react-app"
    ]
  },
  "eslintConfig": {
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended",
      "plugin:import/errors",
      "plugin:import/warnings"
    ],
    "parser": "babel-eslint",
    "parserOptions": {
      "ecmaVersion": 2020,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "env": {
      "browser": true,
      "node": true,
      "es6": true,
      "jest": true
    },
    "rules": {
      "no-debugger": "off",
      "no-console": "off",
      "no-unused-vars": "warn",
      "react/prop-types": "warn"
    },
    "settings": {
      "react": {
        "version": "detect"
      }
    },
    "root": true
  }
}
